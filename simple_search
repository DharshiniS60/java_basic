public class search {
	static int linear_search(int arr[],int search) {
		for(int i=0;i<arr.length;i++) {
			if(arr[i]==search)
				return i;
		}
		return-1;
	}
	public static void main(String []args) {
		int arr[]= {10,35,88,99,55};
		int search=10;
		System.out.print(linear_search(arr,search));
		
	}

}
